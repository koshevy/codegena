{
  "name": "codegena",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "preinstall": "node ./scripts/prevent-npm-install.js",
    "//": "Prebuilt libs help to control and test result of building before publication, but not usuable when you developing",
    "build:all-libs": "ng build oapi3ts && ng build ng-api-service && ng build oapi3ts-cli",
    "clear:prebuilt-libs": "rm -rf dist/*",
    "test:libs": "ng test oapi3ts && ng test ng-api-service && ng test oapi3ts-cli",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "build-and-publish": "./build-and-publish.sh",
    "schematics-example:update": "nx build schematics-example --with-deps && yarn run schematics-example:build-schematics:dev && yarn run schematics-example:link-dist && yarn run schematics-example:clear-result && yarn run schematics-example:generate && yarn",
    "schematics-example:clear-result": "rm -rf libs/schematics-example/src/lib/shop/*",
    "schematics-example:build-schematics:dev": "yarn workspace @codegena/schematics-example run build",
    "schematics-example:link-dist": "yarn workspace @codegena/schematics-example run link:libs:built",
    "schematics-example:generate": "ng g @codegena/schematics-example:backend-services --uri=/DATA/application_projects/codegena/libs/oapi3ts/src/schemas/complex.json --domain=shop --project=schematics-example --secondaryEntrypoint=\"\" --hostModule=\"\" --moduleName=\"shop\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.0",
    "@angular/common": "10.2.0",
    "@angular/compiler": "10.2.0",
    "@angular/core": "10.2.0",
    "@angular/forms": "10.2.0",
    "@angular/platform-browser": "10.2.0",
    "@angular/platform-browser-dynamic": "10.2.0",
    "@angular/router": "10.2.0",
    "@nrwl/angular": "10.3.1",
    "ajv": "^6.10.2",
    "core-js": "^2.5.4",
    "lerna": "^3.20.2",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "rxjs": "6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.7",
    "@angular-devkit/build-ng-packagr": "0.1001.7",
    "@angular/cli": "10.1.7",
    "@angular/compiler-cli": "10.2.0",
    "@angular/language-service": "10.2.0",
    "@nrwl/cli": "10.3.1",
    "@nrwl/cypress": "10.3.1",
    "@nrwl/jest": "10.3.1",
    "@nrwl/workspace": "10.3.1",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "dotenv": "6.2.0",
    "eslint": "^7.7.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "ng-packagr": "10.0.4",
    "prettier": "2.0.4",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.38.1",
    "tslint": "6.0.0",
    "typescript": "4.0.3"
  },
  "workspaces": [
    "apps/todo-app-backend",
    "libs/*"
  ]
}
