{
  "name": "codegena",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "//": "Prebuilt libs help to control and test result of building before publication, but not usuable when you developing",
    "build:all-libs": "node build-all-libs.js",
    "clear:prebuilt-libs": "rm -rf dist/*",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "build-and-publish": "./build-and-publish.sh",
    "todo-app-contract:update": "npm run build:all-libs && npm run schematics-example:build-schematics:dev && npm run schematics-example:link-dist && npm run todo-app-contract:clear-result && npm run todo-app-contract:generate && npm run todo-app-contract:prettier",
    "todo-app-contract:clear-result": "rm -rf ./libs/todo-app-contract/src/lib/todo-app",
    "schematics-example:build-schematics:dev": "npm run --prefix libs/schematics-example build",
    "schematics-example:link-dist": "npm run --prefix libs/schematics-example link:libs:built",
    "todo-app-contract:generate": "ng g @codegena/schematics-example:backend-services --uri=$(pwd)/libs/todo-app-contract/src/oas/app-demo.json --domain=todo-app --project=todo-app-contract --secondaryEntrypoint=\"\" --hostModule=\"\" --moduleName=\"todo-app\" --createSubdir=true",
    "todo-app-contract:prettier": "./node_modules/.bin/prettier --write libs/todo-app-contract/src/lib/todo-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.4",
    "@angular/common": "12.0.4",
    "@angular/compiler": "12.0.4",
    "@angular/core": "12.0.4",
    "@angular/forms": "12.0.4",
    "@angular/platform-browser": "12.0.4",
    "@angular/platform-browser-dynamic": "12.0.4",
    "@angular/router": "12.0.4",
    "@codegena/oapi3ts": "file:dist/oapi3ts",
    "@nrwl/angular": "12.3.6",
    "ajv": "^6.10.2",
    "core-js": "^2.5.4",
    "lerna": "^6.4.1",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "rxjs": "6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.4",
    "@angular/cli": "12.0.4",
    "@angular/compiler-cli": "12.0.4",
    "@angular/language-service": "12.0.4",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "12.3.6",
    "@nrwl/cypress": "12.3.6",
    "@nrwl/jest": "12.3.6",
    "@nrwl/nest": "12.3.6",
    "@nrwl/node": "12.3.6",
    "@nrwl/tao": "12.3.6",
    "@nrwl/workspace": "12.3.6",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "codelyzer": "^6.0.0",
    "dotenv": "8.2.0",
    "eslint": "^7.7.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "26.2.2",
    "jest-preset-angular": "8.4.0",
    "ng-packagr": "12.0.5",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
    "tsickle": "^0.40.0",
    "tslint": "6.1.3",
    "typescript": "4.2.4"
  },
  "workspaces": [
    "libs/*"
  ],
  "engines": {
    "node": ">= 10",
    "npm": ">= 7"
  }
}
