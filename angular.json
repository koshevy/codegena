{
  "version": 1,
  "projects": {
    "todo-app-backend": {
      "root": "apps/todo-app-backend",
      "sourceRoot": "apps/todo-app-backend/src",
      "projectType": "application",
      "prefix": "codegena",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/todo-app-backend",
            "main": "apps/todo-app-backend/src/main.ts",
            "tsConfig": "apps/todo-app-backend/tsconfig.app.json",
            "assets": ["apps/todo-app-backend/src/assets"],
            "externalDependencies": [
              "@nestjs/core",
              "@nestjs/common",
              "@nestjs/microservices",
              "@nestjs/websockets",
              "@nestjs/microservices/microservices-module",
              "@nestjs/websockets/socket-module"
            ],
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/todo-app-backend/src/environments/environment.ts",
                  "with": "apps/todo-app-backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "todo-app-backend:build"
          },
          "port": "3000"
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/todo-app-backend/tsconfig.app.json",
              "apps/todo-app-backend/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/todo-app-backend/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/todo-app-backend/jest.config.js",
            "tsConfig": "apps/todo-app-backend/tsconfig.spec.json"
          }
        }
      }
    },
    "oapi3ts": {
      "projectType": "library",
      "root": "libs/oapi3ts",
      "sourceRoot": "libs/oapi3ts/src",
      "prefix": "codegena",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/oapi3ts/tsconfig.lib.json",
            "project": "libs/oapi3ts/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/oapi3ts/tsconfig.lib.json",
              "libs/oapi3ts/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/oapi3ts/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/oapi3ts/jest.config.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "definitions": {
      "projectType": "library",
      "root": "libs/definitions",
      "sourceRoot": "libs/definitions/src",
      "prefix": "codegena",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/definitions/tsconfig.lib.json",
            "project": "libs/definitions/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/definitions/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/definitions/tsconfig.lib.json",
              "libs/definitions/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/definitions/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/definitions/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    },
    "schematics-example": {
